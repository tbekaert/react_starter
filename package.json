{
  "name": "react_starter",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://tbekaert.github.io/react_starter/",
  "devDependencies": {
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "react-scripts": "^1.1.2",
    "sw-precache": "^5.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chokidar-cli": "^1.2.0",
    "cross-fetch": "^2.1.0",
    "css-mqpacker": "^6.0.2",
    "date-fns": "^1.29.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.2",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.21",
    "postcss-cleaner": "^1.2.2",
    "postcss-cli": "^5.0.0",
    "postcss-inline-svg": "^3.1.1",
    "postcss-normalize": "^4.0.0",
    "query-string": "^5.1.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-ga": "^2.4.1",
    "react-inlinesvg": "^0.8.1",
    "react-localize-redux": "^2.17.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-transition-group": "^2.3.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sanitize.css": "^5.0.0",
    "sort-css-media-queries": "^1.3.4",
    "svg-sprite": "^1.4.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint \"src/**\" --ext .js,.jsx",
    "build:css:postcss": "NODE_ENV=production postcss src/app.css -o src/app.min.css",
    "build:css:sass:app": "node-sass-chokidar src/app.scss -o src/",
    "build:css:sass": "node-sass-chokidar src/ -o src/ --output-style compressed && npm run build:css:sass:app",
    "build:css": "npm-run-all build:css:sass build:css:postcss",
    "build:js": "react-scripts build",
    "build:svg": "node_modules/.bin/svg-sprite --config svg-sprite.config.json 'src/assets/svg/partials/*.svg'",
    "build": "npm-run-all build:css build:svg build:js && sw-precache --config=sw-precache-config.js",
    "start:css:postcss": "postcss src/app.css -w -o src/app.min.css",
    "start:css:sass": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start:css": "npm-run-all -p build:css start:css:sass start:css:postcss",
    "start:js": "react-scripts start",
    "start:svg": "yarn build:svg && chokidar 'src/assets/svg/partials/*.svg' -c 'yarn build:svg'",
    "start": "npm-run-all -p start:css start:js start:svg",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
